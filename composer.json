{
    "name": "arvici/framework",
    "type": "library",
    "description": "Arvici Framework",
    "keywords": [
        "framework",
        "arvici",
        "smart",
        "simple"
    ],
    "homepage": "https://github.com/arvici/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Valk",
            "email": "tomvalk@lt-box.info",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "ext-mbstring": "*",
        "psr/log": "1.0.2",
        "psr/http-message": "1.0.1",
        "monolog/monolog": "1.23.0",
        "mustache/mustache": "2.11.1",
        "doctrine/dbal": "2.5.13",
        "doctrine/annotations": "1.4.0",
        "doctrine/migrations": "1.5.0",
        "doctrine/orm": "2.5.14",
        "indigophp/hash-compat": "1.1.0",
        "swiftmailer/swiftmailer": "6.0.2",
        "symfony/var-dumper": "3.3.16",
        "symfony/console": "3.3.16",
        "symfony/monolog-bridge": "3.3.16",
        "symfony/http-foundation": "3.3.16",
        "symfony/psr-http-message-bridge": "1.0.2",
        "symfony/security": "3.3.16",
        "symfony/validator": "3.3.16",
        "symfony/form": "3.3.16",
        "twig/twig": "2.4.7",
        "tomvlk/sweet-orm": "2.0.0-rc.1",
        "tedivm/stash": "0.14.2",
        "filp/whoops": "2.1.14",
        "maximebf/debugbar": "1.13.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.7",
        "phpunit/php-code-coverage": "5.3.0",
        "satooshi/php-coveralls": "1.1.0",
        "codacy/coverage": "1.4.0"
    },
    "autoload": {
        "psr-4": {
            "Arvici\\": "src/Arvici"
        },
        "files": [
            "src/Arvici/Heart/Log/Logger.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/app/App",
            "SecondApp\\": "tests/app/SecondApp",
            "Arvici\\Tests\\": "tests/src"
        }
    },
    "bin": [
        "bin/arvici"
    ],
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "*phpunit.xml",
            ".travis.yml",
            ".coveralls.yml",
            "composer.phar"
        ]
    }
}
